    "def is_palindrome(word):\n",
    "    word = word.lower()\n",
    "    for i in range(len(word)):\n",
    "        if(i < len(word)/2):\n",
    "            if word[i] != word[-(i+1)]: return False\n",
    "\n",
    "    return True\n"
   ]
